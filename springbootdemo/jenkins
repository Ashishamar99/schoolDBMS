pipeline{
    agent any
    stages{
        stage("compilePhase"){
            steps{
                bat "cd springbootdemo\"
                bat "mvn compile"
                }
            }

        stage("testPhase"){
            steps{
                bat "cd springbootdemo\"
                bat "mvn test"
                }
        }

        stage("packagePhase"){
        steps{
            bat "cd springbootdemo\"
            bat "mvn clean package"
                }
            }
        }
}