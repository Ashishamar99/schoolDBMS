server.port = 9091
spring.jpa.hibernate.ddl-auto=update
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL8Dialect
spring.datasource.url=jdbc:mysql://localhost:3306/school
spring.datasource.username=root

//A project should have either a YAML file or a properties file, not both.
//For a db with no password, delete the whole property as opposed to  spring.datasource.password="". Instead don't have this property itself.
//http://allencoders.online/converters/props2yaml -> Convert Properties to YAML
